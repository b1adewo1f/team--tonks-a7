<html>
<head>
	<title>$tonks - Home</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<!-- Bootstrap -->
	<!-- This is version 3.4.1! -->
	<link href="css/stonks.css" rel="stylesheet">
	<link href="css/bootstrap.css" rel="stylesheet">
	<link href="css/bootstrap-theme.css" rel="stylesheet">
	<!-- jQuery -->
	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="js/bootstrap.js"></script>
</head>
<!-- Main Page -->
<body>
	<!-- Heading -->
	<div class="logo-row">
		<div class="container-fluid">
			<div class="jumbotron">
				<h1>
					<a href="/home">
						<img src="images/logo.svg" alt="stonks" class="logo">
					</a>
				</h1>
			</div>
		</div>
	</div>
	<!-- Heading End -->

	<!-- Dashboard -->
	<div class="panel-group">
		<div class="panel panel-default" id="Intro">
			<div class="panel-heading">
				<h2>Welcome Back!</h2>
			</div>
			<div class="panel-body">
				<p>Welcome back Scott Doe. Let us get you started. </p>
			</div>
		</div>
		<!-- Review Previous Lessons -->
		<!-- This is Panel 1! -->
		<div class="panel panel-default" id="Review">
			<!-- Heading -->
			<div class="panel-heading">
				<h2>Previous Lessons</h2>
			</div>
			<!-- Body -->
			<div class="panel-body">
				<!-- Description -->
				<p>Here are the lessons you've completed!</p>
				<!-- Carousel -->
				<div id="previous-lessons" class="carousel slide" data-ride="carousel">
  				<!-- Indicators -->
					<!-- There are 3 slides per carousel! -->
  				<ol class="carousel-indicators">
    				<li data-target="#previous-lessons" data-slide-to="0" class="active"></li>
    				<li data-target="#previous-lessons" data-slide-to="1"></li>
  				</ol>
					<!-- Wrapper for slides -->
  				<div class="carousel-inner">
						<div class="carousel-inner">
	    				<div class="item active">
	      				<img class="carousel-image" src="images/investing.jpg" alt="investing art">
	      				<div class="carousel-caption">
	        				<p>Investing Introduction</p>
	      				</div>
	    				</div>
							<!-- The remaining .item element can be filled in with JSON -->
							<!-- See the template I e-mailed -->
							<div class="item">
								<img class="carousel-image" src="images/stocks.jpg" alt="stocks">
								<div class="carousel-caption">
									<p>Stocks</p>
								</div>
							</div>
						</div>
  				<!-- Carousel Controls -->
					<!-- The button format is not important right now! -->
  				<a class="left carousel-control" href="#previous-lessons" role="button" data-slide="prev">
    				<span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
    				<span class="sr-only">Previous</span>
  				</a>
  				<a class="right carousel-control" href="#previous-lessons" role="button" data-slide="next">
    				<span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
    				<span class="sr-only">Next</span>
  				</a>
				</div>
			</div>
		</div>
		<!-- End of Panel 1 -->

		<!-- Next Lessons -->
		<!-- This is Panel 2! -->
		<div class="panel panel-default" id="Preview">
			<!-- Heading -->
			<div class="panel-heading">
				<h2>Resume Lessons</h2>
			</div>
			<!-- Body -->
			<div class="panel-body">
				<!-- Description -->
				<p>Here are lessons you haven't completed!</p>
				<!-- Carousel -->
				<div id="next-lessons" class="carousel slide" data-ride="carousel">
  				<!-- Indicators -->
  				<ol class="carousel-indicators">
    				<li data-target="#next-lessons" data-slide-to="0" class="active"></li>
    				<li data-target="#next-lessons" data-slide-to="1"></li>
    				<li data-target="#next-lessons" data-slide-to="2"></li>
  				</ol>
					<!-- Wrapper for slides -->
  				<div class="carousel-inner">
						<div class="item active">
      				<img class="carousel-image" src="images/strategies.jpg" alt="investing strategies">
      				<div class="carousel-caption">
        				<p>Investing Strategies</p>
      				</div>
    				</div>
						<!-- The remaining .item elements can be turned into a handlebars loop! -->
						<!-- See the template I e-mailed -->
						<div class="item">
      				<a href="/lesson-1-graph">
								<img class="carousel-image" src="images/graph.jpeg" alt="graph">
							</a>
      				<div class="carousel-caption">
        				<p>Investing Tools: The Graph</p>
      				</div>
    				</div>
						<div class="item">
      				<img class="carousel-image" src="images/newspaper.jpeg" alt="news">
      				<div class="carousel-caption">
        				<p>Investing Tools: News</p>
      				</div>
    				</div>
  				</div>
  				<!-- Carousel Controls -->
					<!-- The button format is not important right now! -->
  				<a class="left carousel-control" href="#next-lessons" role="button" data-slide="prev">
    				<span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
    				<span class="sr-only">Previous</span>
  				</a>
  				<a class="right carousel-control" href="#next-lessons" role="button" data-slide="next">
    				<span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
    				<span class="sr-only">Next</span>
  				</a>
				</div>
			</div>
		</div>
		<!-- End of Panel 2 -->

		<!-- Recent News About Investing -->
		<!-- This is Panel 3! -->
		<div class="panel panel-default" id="News">
			<!-- Heading -->
			<div class="panel-heading">
				<h2>Changes in Investing</h2>
			</div>
			<!-- Body -->
			<div class="panel-body">
				<!-- Description -->
				<p>The world of investing is always changing!<br>
					 See the news about whats new in investing!
				</p>
				<!-- Carousel -->
				<div id="investing-news" class="carousel slide" data-ride="carousel">
  				<!-- Indicators -->
					<!-- There are 3 slides per carousel! -->
  				<ol class="carousel-indicators">
    				<li data-target="#investing-news" data-slide-to="0" class="active"></li>
    				<li data-target="#investing-news" data-slide-to="1"></li>
    				<li data-target="#investing-news" data-slide-to="2"></li>
  				</ol>
					<!-- Wrapper for slides -->
  				<div class="carousel-inner">
    				<div class="item active">
      				<a href="https://www.wsj.com/" target="_blank">
								<img src="images/WSJ.png" alt="WSJ">
							</a>
    				</div>
						<!-- The remaining .item elements can be turned into a handlebars loop! -->
						<!-- See the template I e-mailed -->
    				<div class="item">
							<a href="https://www.marketwatch.com/investing" target="_blank">
      					<img src="images/marketwatch.png" alt="MarketWatch">
							</a>
						</div>
						<div class="item">
							<a href="https://www.forbes.com/investing/?sh=6db5b17710ba" target="_blank">
      					<img src="images/forbes.jpg" alt="Forbes">
							</a>
    				</div>
  				<!-- Carousel Controls -->
					<!-- The button format is not important right now! -->
  				<a class="left carousel-control" href="#investing-news" role="button" data-slide="prev">
    				<span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
    				<span class="sr-only">Previous</span>
  				</a>
  				<a class="right carousel-control" href="#investing-news" role="button" data-slide="next">
    				<span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
    				<span class="sr-only">Next</span>
  				</a>
				</div>
			</div>
		</div>
		<!-- End of Panel 3 -->
	</div>
	<!-- End of Dashboard -->

	<!-- Navbar -->
	<nav class="navbar navbar-fixed-bottom">
		<div class="container-fluid">
			<ul class="nav nav-tabs nav-justified">
				<!-- Active denotes what page a user is currently on! -->
				<!-- The list below may be a handlebars loop! -->
				<li class="active">
    			<a href="/home">Home</a>
  			</li>
				<li>
    			<a href="/education">Education</a>
  			</li>
				<li>
    			<a href="/investing">Investing</a>
  			</li>
				<li>
					<a href="/support">Support</a>
				</li>
    	</ul>
		</div>
	</nav>
	<!-- If you need help understanding the carousel, go here:
			 https://www.w3schools.com/bootstrap/bootstrap_carousel.asp -->
	<!-- I've completed the carousel in the "education section" so you can see it in action! -->
	<!-- I will add styling and additional lesson pages tomorrow. -->
</body>
</html>
